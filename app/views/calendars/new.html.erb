<h1>新しいカレンダーを作成する</h1>
<%= render 'shared/error_validations', instance: @calendar %>

<%= form_for @calendar do |f| %>
  <p>
    <%= f.label :name %>
  <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :base_hours, '基準時間' %>
  <%= f.text_field :base_hours %>
  </p>
  <%= f.submit %>
<% end %>
